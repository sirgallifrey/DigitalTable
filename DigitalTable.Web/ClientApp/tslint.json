{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-react",
		"tslint-react-hooks"
	],
	"jsRules": {
	  "no-unused-expression": true
	},
	"rules": {
	  "quotemark": [true, "single"],
	  "max-line-length": [true, 150],
	  "indent": [true, "tabs", 4]
	},
	"rulesDirectory": []
  }
  